	keywords = {Automatic control, current angle modulation strategy, Load management, machine vector control, Magnetic flux, maximum torque per ampere, Maximum Torque per Ampere (MTPA), Motion control, MTPA, Optical sensors, permanent magnet motors, Permanent Magnet Synchronous Motor, permanent magnet synchronous motor drives, PMSM drives, point tracking control, Saturation magnetization, stators, synchronous motor drives, Temperature control, torque control, Tracking Control, vector control},
	pages = {24--28},
