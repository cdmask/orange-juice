	keywords = {angular velocity control, Control systems, electric drive speed control, field oriented control, machine vector control, Matrix converters, matrix convertors, Motion control, Permanent magnet machines, permanent magnet motors, Permanent Magnet Synchronous Motor, PI control, PI controller, PMSM matrix converter-fed set up, Pulse width modulation converters, Pulse width modulation inverters, root loci, root locus method, stators, synchronous motor drives, torque control, unwanted windup phenomenon, Voltage, Windup},
	pages = {279--285},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Cdz\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\inleibyv.default\\zotero\\storage\\UFQRCQ44\\5156047.html:text/html}
}

